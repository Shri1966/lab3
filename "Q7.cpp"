#include <iostream>
using namespace std;

int main() {
 char c;
int isuppercasevowel, islowercasevowel;
 cout<<"Enter a alphabet:";
cin>>c;
isuppercasevowel= (c=='A' | c=='B' | c=='C' | c=='D' | c=='E');
islowercasevowel= (c=='a' | c=='b' | c=='c' | c=='d' | c=='e');
if(isuppercasevowel || islowercasevowel)
cout<<"c is vowel";
else
cout<<"c is an consonant";
return 0;
}
